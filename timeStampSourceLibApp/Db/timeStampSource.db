record(longin, "$(DEV):TSS_EVENTCODE")
{
    field(DESC, "Event Code for port $(PORT)")
    field(DTYP, "TimeStampSource")
    field(INP,  "@$(PORT) EventCode")
    field(SCAN, "I/O Intr")
    field(PRIO, "LOW")
    field(TSE, "-2")
    field(MDEL, "-1")
    field(FLNK, "$(DEV):TSS_EVENTCODE_HST")
}

